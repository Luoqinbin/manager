<body>
<input type="hidden" id="csrfName" value="${_csrf.parameterName}"/>
<input type="hidden" id="csrfToken" value="${_csrf.token}"/>
<input type="hidden" id="menuId" value="${menuId}"/>
<div class="portlet box grey-cascade" style="width: 99%">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-gift"></i>查询条件
        </div>
        <div class="tools">
        </div>
    </div>
    <div class="portlet-body form">
        <br>
        <form class="form-horizontal">
            <div class="col-sm-3">
                <label class="control-label col-md-3">名称：</label>
                <div class="input-group">
                    <input class="form-control" placeholder="请输入名称" name="name" id="nameQuery" type="text" autofocus>
                </div>
            </div>
            <button type="button" id="query" class="btn btn-primary"><i class="glyphicon glyphicon-search"></i>查询
            </button>
        </form>
        <br/>
    </div>
</div>
<!-- BEGIN EXAMPLE TABLE PORTLET-->
<div class="portlet box grey-cascade" style="width: 99%">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-globe"></i>列表
        </div>
        <div class="tools">
        </div>
    </div>
    <div class="portlet-body" style="font-size: 14px">
        <div class="table-toolbar">
            <div class="row">
                <div class="col-md-6">
                    ${addBtn}
                    ${listBtn}
                </div>
            </div>
        </div>
        <table class="table table-striped table-bordered table-hover" id="dataTables">
            <thead>
            <tr>
                <th>序号</th>
                <th>储值卡类型</th>
                <th>闲时段折扣率</th>
                <th>忙时段折扣率</th>
                <th>卡额</th>
                <th>卡售价</th>
                <th>提前预定天数</th>
                <th>有效期（月）</th>
                <th>可售状态</th>
            </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>
<!-- END EXAMPLE TABLE PORTLET-->

<div id="addWin" style="display: none;width: 99%" class="portlet-body form">
    <form role="form" action="memberCard/addOrUpdate" method="post" id="form" class="form-horizontal form-validate">
        <input type="hidden" value="${_csrf.token}" name="${_csrf.parameterName}">
        <input type="hidden" value="" name="id" id="userId">
        <div class="form-body">
            <div class="form-group">
                <label class="control-label col-md-3">储值卡类型： <span class="required">* </span>
                </label>
                <div class="col-md-8">
                    <input type="text" class="form-control" data-required="1" id="name" name="name"
                           placeholder="请输入储值卡类型">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">卡前缀：<span class="required">* </span>
                </label>
                <div class="col-md-8">
                    <input type="text" class="form-control" name="rank" id="rank" placeholder="请输入卡前缀">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">闲时折扣(折)：
                </label>
                <div class="col-md-8">
                    <input type="text" class="form-control" name="emptyDiscount" id="emptyDiscount" placeholder="请输入">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">忙时折扣(折)：
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="busyDiscount" id="busyDiscount" placeholder="请输入">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">卡金额：
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="titleAccount" id="titleAccount" placeholder="请输入">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">卡售价：
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="price" id="price" placeholder="请输入">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">提前预定天数：
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="payDate" id="payDate" placeholder="请输入">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">有效期(月)：
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="last" id="last" placeholder="请输入">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">状态：
                </label>
                <div class="col-md-8">
                    <select class="form-control" name="status" id="status">
                        <option value="">请选择</option>
                       <option value="1">可售</option>
                        <option value="2">不可售</option>
                    </select>
                </div>
            </div>
        </div>
    </form>
</div>

<!-- END EXAMPLE TABLE PORTLET-->
