<input type="hidden" id="menuId" value="${menuId}"/>
<div class="portlet box grey-cascade" style="width: 99%">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-gift"></i>查询条件
        </div>
        <div class="tools">
        </div>
    </div>
    <div class="portlet-body form">
        <br>
        <form class="form-horizontal">
            <div class="col-sm-3">
                <label class="control-label col-md-3">楼层：</label>
                <div class="input-group">
                    <select class="select"  name="area" id="areaQuery" style="width: 200px">
                        <option value="">请选择楼层</option>
                        #foreach($item in $areaList)
                            <option value="$item">$item</option>
                        #end
                    </select>
                </div>
            </div>
            <button type="button" id="query" class="btn btn-primary"><i class="glyphicon glyphicon-search"></i>查询
            </button>
        </form>
        <br/>
    </div>
</div>
<!-- BEGIN EXAMPLE TABLE PORTLET-->
<div class="portlet box grey-cascade" style="width: 99%">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-globe"></i>列表
        </div>
        <div class="tools">
        </div>
    </div>
    <div class="portlet-body" style="font-size: 14px">
        <div class="table-toolbar">
            <div class="row">
                <div class="col-md-6">
                    ${addBtn}
                    ${listBtn}
                </div>
            </div>
        </div>
        <table class="table table-striped table-bordered table-hover" id="dataTables">
            <thead>
            <tr>
                <th>楼层</th>
                <th>场地</th>
                <th>忙时价</th>
                <th>闲时价</th>
                <th>忙时现金价</th>
                <th>闲时现金价</th>
            </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>
<!-- END EXAMPLE TABLE PORTLET-->

<div id="addWin" style="display: none;width: 99%" class="portlet-body form">
    <form role="form" action="courtInfo/addOrUpdate" method="post" id="form" class="form-horizontal form-validate">
        <input type="hidden" value="${_csrf.token}" name="${_csrf.parameterName}">
        <input type="hidden" value="" name="id" id="userId">
        <div class="form-body">
            <div class="form-group">
                <label class="control-label col-md-3">场地名称(号场)： <span class="required">* </span>
                </label>
                <div class="col-md-8">
                    <input type="text" class="form-control" id="name" name="name"
                           placeholder="请输入场地名称">
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-md-3">楼层(F)： <span class="required">* </span>
                </label>
                <div class="col-md-8">
                    <input type="text" class="form-control" name="area" id="area" placeholder="请输入楼层">
                </div>
            </div>
            <div class="form-group" id="passwordPwd">
                <label class="control-label col-md-3">忙时价： <span class="required">* </span>
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="busyBasePrice" id="busyBasePrice" placeholder="请输入忙时价">
                </div>
            </div>
            <div class="form-group" id="passwordPwd">
                <label class="control-label col-md-3">闲时价： <span class="required">* </span>
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="emptyBasePrice" id="emptyBasePrice" placeholder="请输入闲时价">
                </div>
            </div>
            <div class="form-group" id="passwordPwd">
                <label class="control-label col-md-3">忙时现金价： <span class="required">* </span>
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="busyCashPrice" id="busyCashPrice" placeholder="请输入忙时现金价">
                </div>
            </div>
            <div class="form-group" id="passwordPwd">
                <label class="control-label col-md-3">闲时现金价： <span class="required">* </span>
                </label>
                <div class="col-md-8">
                    <input class="form-control" name="emptyCashPrice" id="emptyCashPrice" placeholder="请输入闲时现金价">
                </div>
            </div>
        </div>
    </form>
</div>

