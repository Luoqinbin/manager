<script type="text/javascript">
    jQuery(document).ready(function () {
        TableAdvanced.init();
        $(".select").select2({
            placeholder: "请选择",
            val:""
        });
        require(["modules/court/fixedOrderList"], function (view) {
            view.initPage();
        });
    });
